{
  "name": "team_picker",
  "version": "0.0.1",
  "description": "team picker that stores list of cohorts and is able to generate teams for that cohort.",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "createdb --echo teampicker",
    "db:drop": "dropdb --if-exists --echo teampicker",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate"

  },
  "keywords": [
    "express",
    "node",
    "npm",
    "knex",
    "bootstrap",
    "ejs",
    "morgon"
  ],
  "author": "Arivin Nair",
  "license": "ISC",
  "dependencies": {
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "knex": "^0.16.3",
    "morgan": "^1.9.1",
    "pg": "^7.8.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
